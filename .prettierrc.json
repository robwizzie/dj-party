{
    "tabWidth": 4,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "strict",
    "quoteProps": "consistent",
    "singleQuote": true,
    "trailingComma": "none",
    "useTabs": true,
    "printWidth": 130,
    "bracketSpacing": true,
    "operatorLinebreak": "before",
    "bracketSameLine": true,
    "insertPragma": false,
    "proseWrap": "preserve"
}